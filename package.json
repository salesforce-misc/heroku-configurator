{
  "name": "heroku-configurator",
  "version": "0.0.0",
  "description": "oclif example Hello World CLI",
  "author": "Demian Brecht @demianbrecht",
  "bin": {
    "heroku-configurator": "./bin/run"
  },
  "homepage": "https://github.com/demianbrecht/hello-world",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "demianbrecht/hello-world",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@heroku-cli/command": "^8.5.0",
    "@heroku-cli/schema": "^1.0.25",
    "@oclif/command": "^1.8.18",
    "@oclif/core": "^1.19.1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.1.4",
    "deep-object-diff": "^1.1.7",
    "table": "^6.8.1",
    "tmp": "^0.2.1",
    "yaml": "^2.1.3",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@oclif/test": "^2.2.10",
    "@types/chai": "^4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.68",
    "@types/tmp": "^0.2.3",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "mocha": "^10.1.0",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.3.1",
    "tsup": "^6.4.0",
    "typescript": "^4.8.4"
  },
  "oclif": {
    "bin": "heroku-configurator",
    "dirname": "heroku-configurator",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      },
      "configurator": {
        "description": "Does magical stuff"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsup",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/demianbrecht/hello-world/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
